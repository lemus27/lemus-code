<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">

    <jsp:directive.page contentType="text/html" pageEncoding="UTF-8"/>

    <!-- any content can be specified here, e.g.: -->
    <jsp:element name="text">
        <jsp:attribute name="lang">EN</jsp:attribute>
        <jsp:body>Hello World!</jsp:body>
        <H3>
            <hr ></hr>
            pï¿½gina HTML de Applet
            <hr ></hr>
        </H3>
        <title>Accessing JavaScript from an applet</title>
        <form NAME="textForm">
            <P>Enter some text and then click Display Text:
                <input TYPE="text" NAME="textField" SIZE="20">
                </input>
            </P>
        </form>
        <P>
             <jsp:plugin
                 type="applet"
                 codebase="classes"
                 code="WEB-INF/classes/applets/ReadForm.class"
                 width="350"
                 height="200"
                 >
        </jsp:plugin>
        </P>

        <hr ></hr>
        <FONT SIZE="-1">
            <I>
                generado por el IDE NetBeans
            </I>
        </FONT>
        <hr ></hr>

    </jsp:element>

</jsp:root>
